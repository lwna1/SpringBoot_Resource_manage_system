<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.teamwork.DAO.UserMapper">

    <select id="getTheUser" resultType="com.teamwork.entity.User">
        select * from teamwork.user where userId = #{id}
    </select>

    <select id="getAllUser" resultType="com.teamwork.entity.User">
        select * from teamwork.user
    </select>

</mapper>